{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "errors/error-types": "<rootDir>/../libs/error-types/src",
    "errors/error-types/(.*)": "<rootDir>/../libs/error-types/src/$1"
  }
}