{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "error-types": {
      "type": "library",
      "root": "libs/error-types",
      "entryFile": "index",
      "sourceRoot": "libs/error-types/src",
      "compilerOptions": {
        "tsConfigPath": "libs/error-types/tsconfig.lib.json",
        "webpack": false
      }
    }
  }
}
